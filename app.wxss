/**app.wxss**/
@import '/dist/style/weui.wxss';

page{
  height: initial !important;
  font-family:PingFangSC-Medium,PingFang SC;
}
pages{
  position: relative;
  z-index: 9999998;
  background-image: linear-gradient(135deg,rgba(62,179,255,1) 0%,rgba(0,139,255,1) 100%);
}

button {
  padding-top: 0;
  background: none;
  border-radius: 0;
}

button::after{
  border: none;
}